{
  "name": "Fixate",
  "description": "A simple, reusable, web app wrapper for Chrome OS.",
  "version": "1.1.0",
  "manifest_version": 2,
  "app": {
    "background": {
      "scripts": ["js/background.js"]
    }
  },
  "commands": {
    "exit-app": {
      "suggested_key": {
        "default": "Ctrl+Shift+L"
      },
      "description": "Exit the app"
    }
  },
  "icons": {
      "16": "img/aca-16.png",
      "128": "img/aca-128.png"
  },
  "kiosk_enabled" : true,
  "permissions": [
    "power",
    "webview",
    "storage",
    "app.window.fullscreen",
    "app.window.fullscreen.overrideEsc"
  ]
}
