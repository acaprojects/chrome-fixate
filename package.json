{
  "name": "chrome-fixate",
  "version": "1.1.0",
  "main": "src/js/background.js",
  "description": "A simple, reusable, web app wrapper for Chrome OS",
  "author": "Kim Burgess <kim@acaprojects.com>",
  "license": "MIT",
  "scripts": {
    "start": "gulp"
  },
  "eslintConfig": {
    "extends": "eslint:recommended",
    "env": {
      "es6": true,
      "node": true,
      "browser": true
    },
    "globals": {
      "chrome": true
    },
    "rules": {
      "no-console": 0,
      "semi": 2
    },
    "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module"
    }
  },
  "babel": {
    "presets": [
      "latest"
    ]
  },
  "dependencies": {
    "babel-core": "^6.24.1",
    "babel-loader": "^7.0.0",
    "babel-preset-latest": "^6.24.1",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.28.0",
    "del": "^2.2.2",
    "eslint": "^3.19.0",
    "extract-text-webpack-plugin": "^2.1.0",
    "gulp": "^3.9.1",
    "gulp-eslint": "^3.0.1",
    "gulp-util": "^3.0.8",
    "gulp-zip": "^4.0.0",
    "html-webpack-plugin": "^2.28.0",
    "node-sass": "^4.5.2",
    "sass-loader": "^6.0.3",
    "style-loader": "^0.17.0",
    "url-loader": "^0.5.8",
    "valid-url": "^1.0.9",
    "webpack": "^2.3.3"
  }
}
